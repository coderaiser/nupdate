{
  "name": "nupdate",
  "version": "7.1.1",
  "description": "update node module dependency in package.json",
  "main": "lib/nupdate.js",
  "bin": {
    "nupdate": "bin/nupdate.js"
  },
  "scripts": {
    "lint": "redrun lint:*",
    "lint:eslint": "eslint lib test",
    "lint:eslint:bin": "eslint --rule 'no-console:0' bin",
    "test": "tape test/*.js",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "watcher": "nodemon -w test -w lib --exec",
    "watch:test": "npm run watcher -- npm test"
  },
  "dependencies": {
    "currify": "^3.0.0",
    "detect-indent": "^6.0.0",
    "find-up": "^4.1.0",
    "finicky": "^1.0.0",
    "for-each-key": "^1.0.1",
    "fullstore": "^1.0.0",
    "jessy": "^2.0.1",
    "jonny": "^2.0.3",
    "minimist": "^1.2.0",
    "nessy": "^2.1.0",
    "sorted-object": "^2.0.1",
    "try-catch": "^2.0.0",
    "wraptile": "^2.0.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^5.1.0",
    "eslint-plugin-node": "^8.0.0",
    "nodemon": "^1.11.0",
    "nyc": "^14.1.1",
    "redrun": "^7.0.0",
    "tape": "^4.6.3"
  },
  "engines": {
    "node": ">=8.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coderaiser/nupdate"
  },
  "keywords": [
    "npm",
    "update",
    "dependency"
  ],
  "author": "coderaiser <mnemonic.enemy@gmail.com> (http://coderaiser.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coderaiser/nupdate/issues"
  },
  "homepage": "https://github.com/coderaiser/nupdate",
  "publishConfig": {
    "access": "public"
  }
}
