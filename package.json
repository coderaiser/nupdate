{
  "name": "nupdate",
  "version": "4.1.2",
  "description": "update node module dependency in package.json",
  "main": "lib/nupdate.js",
  "bin": {
    "nupdate": "bin/nupdate.js"
  },
  "scripts": {
    "lint": "redrun lint:*",
    "lint:eslint": "eslint lib",
    "lint:eslint:bin": "eslint --rule 'no-console:0' bin",
    "prepublish": "rm -f package-lock.json",
    "postpublish": "npm i -g",
    "test": "tape test/*.js",
    "coverage": "nyc npm test",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "watcher": "nodemon -w test -w lib --exec",
    "watch:test": "npm run watcher -- npm test"
  },
  "dependencies": {
    "currify": "^2.0.4",
    "detect-indent": "^5.0.0",
    "es6-promisify": "^5.0.0",
    "jessy": "^2.0.1",
    "jonny": "^1.0.1",
    "minimist": "^1.2.0",
    "nessy": "^2.0.0",
    "try-catch": "^1.0.0"
  },
  "devDependencies": {
    "coveralls": "^2.13.1",
    "eslint": "^4.0.0",
    "nodemon": "^1.11.0",
    "nyc": "^11.0.2",
    "tape": "^4.6.3"
  },
  "engines": {
    "node": ">=4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/coderaiser/nupdate"
  },
  "keywords": [
    "npm",
    "update",
    "dependency"
  ],
  "author": "coderaiser <mnemonic.enemy@gmail.com> (http://coderaiser.github.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coderaiser/nupdate/issues"
  },
  "homepage": "https://github.com/coderaiser/nupdate",
  "false": {}
}